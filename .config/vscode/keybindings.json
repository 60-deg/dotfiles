// 既定値を上書きするには、このファイル内にキー バインドを挿入しますauto[]
[
  {
    "key": "ctrl+alt+right",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "ctrl+alt+down",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "ctrl+alt+left",
    "command": "workbench.action.splitEditorLeft"
  },
  {
    "key": "ctrl+alt+up",
    "command": "workbench.action.splitEditorUp"
  },
  {
    "key": "shift+cmd+p",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "shift+cmd+p",
    "command": "workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "cmd+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "shift+cmd+p",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "alt+2",
    "command": "workbench.action.editorLayoutTwoColumns"
  },
  {
    "key": "shift+alt+2",
    "command": "workbench.action.editorLayoutTwoRows"
  },
  {
    "key": "alt+cmd+p",
    "command": "markdown.showPreviewToSide",
    "when": "!terminalFocus && editorLangId == 'markdown'"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.showPreviewToSide",
    "when": "editorLangId == 'markdown'"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.extension.togglePreviewToSide",
    "when": "!terminalFocus"
  },
  {
    "key": "cmd+k p",
    "command": "workbench.action.files.copyPathOfActiveFile"
  },
  {
    "key": "cmd+k p",
    "command": "-workbench.action.files.copyPathOfActiveFile"
  },
  {
    "key": "alt+cmd+p",
    "command": "graphviz.showPreviewToSide",
    "when": "editorLangId == 'dot'"
  },
  {
    "key": "cmd+k v",
    "command": "-graphviz.showPreviewToSide",
    "when": "editorLangId == 'dot'"
  },
  {
    "key": "ctrl+shift+alt+right",
    "command": "-cursorWordPartRightSelect",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+left",
    "command": "-cursorWordPartLeftSelect",
    "when": "textInputFocus"
  },
  {
    "key": "alt+cmd+down",
    "command": "workbench.action.nextSideBarView"
  },
  {
    "key": "alt+cmd+up",
    "command": "workbench.action.previousSideBarView"
  },
  {
    "key": "ctrl+b",
    "command": "-cursorLeft",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+f",
    "command": "-cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+b b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+,",
    "command": "workbench.action.openSettingsJson"
  },
  {
    "key": "alt+cmd+,",
    "command": "workbench.action.openSettings"
  },
  {
    "key": "cmd+,",
    "command": "-workbench.action.openSettings"
  },
  {
    "key": "alt+cmd+down",
    "command": "-editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+down",
    "command": "-workbench.action.terminal.focusNextPane",
    "when": "terminalFocus"
  },
  {
    "key": "alt+cmd+up",
    "command": "-workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus"
  },
  {
    "key": "alt+cmd+up",
    "command": "-editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+b",
    "command": "latex-workshop.build",
    "when": "resourceLangId == latex"
  },
  {
    "key": "ctrl+alt+c",
    "command": "latex-workshop.clean",
    "when": "resourceLangId == latex"
  },
  {
    "key": "ctrl+alt+s",
    "command": "latex-workshop.synctex",
    "when": "resourceLangId == latex"
  },
  {
    "key": "alt+c right",
    "command": "lastCursorWordSelect"
  },
  {
    "key": "cmd+l",
    "command": "expandLineSelection"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFileFolder"
  },
  {
    "key": "shift+cmd+u",
    "command": "-workbench.action.output.toggleOutput",
    "when": "workbench.panel.output.active"
  },
  {
    "key": "shift+cmd+u",
    "command": "cursorRedo"
  }
]
