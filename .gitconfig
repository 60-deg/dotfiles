[core]
	excludesfile = /Users/hatsumi/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = 60deg
	email = hatsumi.yamane.4609@gmail.com
[commit]
	template = /Users/hatsumi/.stCommitMsg
[color]
	ui = auto
	#色の表示ができないとき
	#ui = false
[alias]
	st = status
	cm = commit
	cmm = commit -m
	a = add
	df = diff
	co = checkout
	br = branch
	lg = log
	gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %C(blue)%cn %C(red)%d %Creset%s'
	gra = log --graph --date=short --decorate=short --all --pretty=format:'%Cgreen%h %Creset%cd %C(blue)%cn %C(red)%d %Creset%s'
	po = commit -m "po"
	pushsu = push --set-upstream
	pushsuo = push --set-upstream origin
